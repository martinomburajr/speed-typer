package model;

public class Difficulty {
	
	private final static int EASY = 3;
	private final static int INTERMEDIATE = 6;
	private final static int HARD = 11;
	private final static int ASIAN = 17;
	private final static int SUPERSAIYAN = 30;
	
	public Difficulty()
	{
		
	}
	
	public static int getEasy() {
		return EASY;
	}

	public static int getIntermediate() {
		return INTERMEDIATE;
	}

	public static int getHard() {
		return HARD;
	}

	public static int getAsian() {
		return ASIAN;
	}

	public static int getSuperSaiyan() {
		return SUPERSAIYAN;
	}

	
	
}
